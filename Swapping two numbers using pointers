#include<stdio.h>
#include<conio.h>

int main()
{
    int x,y,*a,*b,temp;

    printf("Enter values of x and y :");
    scanf("%d%d",&x,&y);

    printf("Before swapping :");
    printf("\nx=%d \ny=%d\n",x,y);

    a=&x;
    b=&y;

    temp=*b;
    b*=a*
    a*=temp;
    printf("After swapping :");
    printf("\nx=%d \ny=%d\n",x,y);

    return 0;
}
